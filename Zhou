@Test
		public void testSetAdvisorAssert() {
			PHD person0 = new PHD("Lei Zhou",'m', 2016, 5);
			PHD person1 = new PHD("Yingwen Cai",'f', 1990, 8);
			PHD person2 = new PHD("Hao Rong",'m', 2000, 6);
			PHD person4 = new PHD("Zongze Guo",'m', 2018, 5);
			
			try {
				person0.setAdvisor1(null); // assert variable is not null
				person0.setAdvisor1(person1);
				person0.setAdvisor1(person2); //assert advisor1 is unknown
		        fail("The assert codes are not working properly.");
		    }
		    catch (AssertionError e) {
		        System.out.println("Caught " + e.getMessage());
		    }
			
			try {
				person0.setAdvisor1(person1);
				person0.setAdvisor2(null); // assert variable is not null
				person0.setAdvisor2(person1); // assert advisor2 is different from advisor1
				person0.setAdvisor2(person2);
				person0.setAdvisor2(person4); // assert advisor2 is unknown
		        fail("The assert codes are not working properly.");
		    }
		    catch (AssertionError e) {
		        System.out.println("Caught " + e.getMessage());
		    }
		}
