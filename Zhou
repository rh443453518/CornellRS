///Test case group of D
		@Test
		public void testCompare() {
			//Test gotBefore
			PHD person0 = new PHD("Lei Zhou",'m', 2016, 5);
			PHD person1 = new PHD("Yingwen Cai",'f', 1990, 8);
			PHD person2 = new PHD("Hao Rong",'m', 2000, 6);
			PHD person4 = new PHD("Zongze Guo",'m', 2018, 5);
			assertEquals(person0.gotBefore(person2), false);
			assertEquals(person1.gotBefore(person2), true);
			
			//Add more advisee to an advisor
			person0.setAdvisor1(person2);
			person1.setAdvisor1(person2);
			assertEquals(person0.arePHDSiblings(person1), true);
			assertEquals(person0.arePHDSiblings(person0), false);
			assertEquals(person0.arePHDSiblings(person2), false);
		} 
